   "source": [
    "from crontab import CronTab\n",
    "\n",
    "cron = CronTab(tab=\"\"\"* * * * * command\"\"\")\n",
    "job = cron.new(command='python print(1)')  \n",
    "job.minute.every(1)\n",
    "\n",
    "cron.write()"
   ]
  }
